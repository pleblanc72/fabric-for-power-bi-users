/// Targets table
/// Monthly revenue and transaction targets by store

table Targets
    lineageTag: targets-001

    column StoreID
        dataType: string
        lineageTag: tgt-storeid

    column Year
        dataType: int64
        lineageTag: tgt-year

    column Month
        dataType: int64
        lineageTag: tgt-month

    column RevenueTarget
        dataType: decimal
        formatString: "$#,0.00"
        lineageTag: tgt-revenue

    column TransactionTarget
        dataType: int64
        formatString: #,0
        lineageTag: tgt-txn
