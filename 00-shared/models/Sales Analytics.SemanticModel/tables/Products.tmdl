/// Products dimension table
/// ~65 products across 8 categories of outdoor and sporting goods

table Products
    lineageTag: products-001

    column ProductID
        dataType: string
        isKey: true
        lineageTag: prod-id

    column ProductName
        dataType: string
        lineageTag: prod-name

    column Category
        dataType: string
        lineageTag: prod-cat

    column SubCategory
        dataType: string
        lineageTag: prod-subcat

    column UnitPrice
        dataType: decimal
        formatString: "$#,0.00"
        lineageTag: prod-price

    column UnitCost
        dataType: decimal
        formatString: "$#,0.00"
        lineageTag: prod-cost

    column IsActive
        dataType: boolean
        lineageTag: prod-active

    hierarchy 'Product Hierarchy'
        lineageTag: prod-hier

        level Category
            column: Category

        level SubCategory
            column: SubCategory

        level ProductName
            column: ProductName
